<%- include("partials/header"); -%>
<h1>COUNTRY LEVEL UPDATES</h1>
<table class="table table-striped">
    <thead>
      <tr>
        <th scope="col">SL NO.</th>
        <th scope="col">Country</th>
        <th scope="col">New Confirmed Cases</th>
        <th scope="col">Total Confirmed Cases</th>
        <th scope="col">New Deaths</th>
        <th scope="col">Total Deaths</th>
        <th scope="col">New Recoverd Cases</th>
        <th scope="col">Total Recoverd Cases</th>
      </tr>
    </thead>
    <tbody>
      <% let i =0; %>
      <% countryData.forEach(function(data) { %>
        <% i = i + 1 %>
        <tr>
            <th scope="row"><%= i %></th>
            <td><%= data.Country %></td>
            <td><%= data.NewConfirmed %></td>
            <td><%= data.TotalConfirmed %></td>
            <td><%= data.NewDeaths %></td>
            <td><%= data.TotalDeaths %></td>
            <td><%= data.NewRecovered %></td>
            <td><%= data.TotalRecovered %></td>
        </tr>
      <%})%>
    </tbody>
  </table>



<%- include("partials/footer"); -%>